@model OrderManagementMVC.ViewModels.OrderTraceView

@{
    ViewData["Title"] = "ViewTraces";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>ViewTraces</h1>

<div>
    <h4>ViewTraces</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.OrderNumber)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.OrderNumber)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ClientName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ClientName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Quantity)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Quantity)
        </dd>
    </dl>
</div>
<div>
    
    <a asp-action="Index">Back to List</a>
</div>
<label asp-for="orderTraceModels" class="control-label">Order traces</label>

@foreach (var item in Model.orderTraceModels)
{
    <div style="border: 1px solid blue;">

        <p>
            @Html.DisplayFor(modelItem => item.IdBoxNumber)
            @Html.ActionLink("Edit", "Edit", new { /*orderNumber = Model.OrderNumber */}) 
        </p>
    </div>
}
